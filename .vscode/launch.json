  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next: Launch",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "profile": "dev-edition-default",
      "pathMappings": [
        {
          "url": "webpack://_n_e/src",
          "path": "${workspaceFolder}/src"
        },
        {
          "url": "http://127.0.0.1:3000/_next/",
          "path": "${webRoot}/.next/"
        }
      ]
    }
  ]
}
